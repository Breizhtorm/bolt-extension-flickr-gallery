{
    "name": "breizhtorm/flickrgallery",
    "description": "A simple Flickr images gallery.",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": ">=2.0.0,<3.0.0"
    },
    "keywords" : [
        "Flickr",
        "image",
        "gallery"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Breizhtorm", 
            "email": "welcome@breizhtorm.fr", 
            "homepage": "http://www.breizhtorm.fr"
        }
    ],
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\Breizhtorm\\FlickrGallery\\" : [
                "",
                "src/"
            ]
        }
    },
    
    "extra": {
        "bolt-assets": "assets",
        "bolt-icon": "assets/icon-flickrgallery.png",
    }
}
